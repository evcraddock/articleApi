{
  "name": "mustangnewsservice",
  "version": "1.0.0",
  "description": "",
  "main": "src",
  "scripts": {
    "start": "node .",
    "clean": "del 'dist/!(.git*|Procfile)**'",
    "copy": "copy package.json dist && copy src/**/package.json dist/src && copy manifest.yml dist",
    "transpile": "babel src -d dist/src -s inline -i 'src/**/*.test.js'",
    "build": "npm run clean && npm run copy && npm run transpile"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ]
  },
  "author": "Erik Craddock",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "copy": "^0.3.0",
    "del-cli": "^0.2.0",
    "dotenv-safe": "^2.3.2"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "crypto": "0.0.3",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.16.4",
    "mongoose": "^4.6.2",
    "mongoose-keywords": "^0.3.1",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^2.1.0"
  }
}
